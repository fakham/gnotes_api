{
  "db": {
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "url": "${DB_URL}",
    "database": "${DB_NAME}",
    "password": "${DB_PASSWORD}",
    "name": "db",
    "user": "${DB_USER}",
    "useNewUrlParser": true,
    "connector": "mongodb"
  },
  "assets": {
    "name": "assets",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "./assets",
    "nameConflict": "makeUnique"
  }
}
